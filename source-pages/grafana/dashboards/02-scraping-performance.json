{
  "title": "Playwright Scraping - Performance",
  "tags": [
    "playwright",
    "scraping",
    "performance"
  ],
  "timezone": "browser",
  "schemaVersion": 27,
  "version": 1,
  "refresh": "10s",
  "panels": [
    {
      "id": 1,
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 0
      },
      "type": "timeseries",
      "title": "Taxa de Extração de Livros",
      "targets": [
        {
          "expr": "rate(books_scraped_total[5m])",
          "refId": "A",
          "legendFormat": "Livros/segundo",
          "datasource": {
            "type": "prometheus",
            "uid": "prometheus"
          }
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "palette-classic"
          },
          "custom": {
            "drawStyle": "line",
            "lineInterpolation": "smooth",
            "lineWidth": 2,
            "fillOpacity": 20,
            "gradientMode": "opacity",
            "spanNulls": false,
            "pointSize": 5
          },
          "unit": "short",
          "decimals": 2
        }
      }
    },
    {
      "id": 2,
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 0
      },
      "type": "timeseries",
      "title": "Throughput (Livros por Segundo)",
      "targets": [
        {
          "expr": "books_scraped_per_second",
          "refId": "A",
          "legendFormat": "Throughput",
          "datasource": {
            "type": "prometheus",
            "uid": "prometheus"
          }
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "palette-classic"
          },
          "custom": {
            "drawStyle": "bars",
            "lineWidth": 1,
            "fillOpacity": 80
          },
          "unit": "short",
          "decimals": 2
        }
      }
    },
    {
      "id": 3,
      "gridPos": {
        "h": 8,
        "w": 24,
        "x": 0,
        "y": 8
      },
      "type": "timeseries",
      "title": "Total Acumulado de Livros Extraídos",
      "targets": [
        {
          "expr": "sum(books_scraped_total)",
          "refId": "A",
          "legendFormat": "Total de Livros",
          "datasource": {
            "type": "prometheus",
            "uid": "prometheus"
          }
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "palette-classic"
          },
          "custom": {
            "drawStyle": "line",
            "lineWidth": 2,
            "fillOpacity": 30,
            "gradientMode": "opacity"
          },
          "unit": "short"
        }
      }
    },
    {
      "id": 4,
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 16
      },
      "type": "timeseries",
      "title": "Requisições de Paginação",
      "targets": [
        {
          "expr": "sum(rate(pagination_requests_total[5m])) by (page_number)",
          "refId": "A",
          "legendFormat": "Página {{page_number}}",
          "datasource": {
            "type": "prometheus",
            "uid": "prometheus"
          }
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "palette-classic"
          },
          "custom": {
            "drawStyle": "line",
            "lineWidth": 2
          },
          "unit": "short"
        }
      }
    },
    {
      "id": 5,
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 16
      },
      "type": "bargauge",
      "title": "Total de Requisições por Página",
      "targets": [
        {
          "expr": "sum(pagination_requests_total) by (page_number)",
          "refId": "A",
          "legendFormat": "Página {{page_number}}",
          "datasource": {
            "type": "prometheus",
            "uid": "prometheus"
          }
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "palette-classic"
          },
          "custom": {
            "displayMode": "gradient",
            "orientation": "horizontal"
          },
          "unit": "short"
        }
      },
      "options": {
        "displayMode": "gradient",
        "orientation": "horizontal",
        "showUnfilled": true
      }
    }
  ],
  "time": {
    "from": "now-1h",
    "to": "now"
  },
  "timepicker": {},
  "templating": {
    "list": []
  },
  "annotations": {
    "list": []
  },
  "editable": true,
  "fiscalYearStartMonth": 0,
  "graphTooltip": 0,
  "links": [],
  "liveNow": false,
  "weekStart": ""
}